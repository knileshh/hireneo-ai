{
  "name": "hireneo-ai",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=24.12.0",
    "npm": ">=11.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "worker": "tsx scripts/worker.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "docker:build:app": "docker build -t hireneo-app .",
    "docker:build:worker": "docker build -f Dockerfile.worker -t hireneo-worker .",
    "docker:build": "npm run docker:build:app && npm run docker:build:worker",
    "docker:run:app": "docker run -p 3000:3000 --env-file .env hireneo-app",
    "docker:run:worker": "docker run --env-file .env hireneo-worker",
    "docker:push:app": "docker push yourusername/hireneo-app:latest",
    "docker:push:worker": "docker push yourusername/hireneo-worker:latest",
    "docker:push": "npm run docker:push:app && npm run docker:push:worker",
    "prepare": "husky"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.0.10",
    "@polar-sh/nextjs": "^0.9.3",
    "@polar-sh/sdk": "^0.42.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.89.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query": "^5.62.9",
    "ai": "^4.0.22",
    "bullmq": "^5.34.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.38.2",
    "framer-motion": "^12.23.26",
    "ioredis": "^5.4.2",
    "lucide-react": "^0.562.0",
    "mammoth": "^1.11.0",
    "next": "16.1.1",
    "pdf-parse": "^2.4.5",
    "pg": "^8.13.1",
    "pino": "^9.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "resend": "^4.0.3",
    "tailwind-merge": "^3.4.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "drizzle-kit": "^0.30.1",
    "eslint": "^9.17.0",
    "eslint-config-next": "16.1.1",
    "husky": "^9.1.7",
    "pino-pretty": "^13.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
